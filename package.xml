<assembly xmlns="http://maven.apache.org/ASSEMBLY/2.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/ASSEMBLY/2.0.0 http://maven.apache.org/xsd/assembly-2.0.0.xsd">
    <id>parent</id>
    <formats>
        <format>dir</format>
    </formats>
    <includeBaseDirectory>false</includeBaseDirectory>
    <fileSets>
        <fileSet>
            <directory>${project.basedir}/MavenCommon/target</directory>
            <outputDirectory>common</outputDirectory>
            <includes>
                <include>**/*.*</include>
            </includes>
            <fileMode>0755</fileMode>
        </fileSet>
        <fileSet>
            <directory>${project.basedir}/MavenConfig/target</directory>
            <outputDirectory>config</outputDirectory>
            <includes>
                <include>**/*.*</include>
            </includes>
            <fileMode>0755</fileMode>
        </fileSet>
        <fileSet>
            <directory>${project.basedir}/MavenEntity/target</directory>
            <outputDirectory>entity</outputDirectory>
            <includes>
                <include>**/*.*</include>
            </includes>
            <fileMode>0755</fileMode>
        </fileSet>
        <fileSet>
            <directory>${project.basedir}/MavenRepository/target</directory>
            <outputDirectory>repository</outputDirectory>
            <includes>
                <include>**/*.*</include>
            </includes>
            <fileMode>0755</fileMode>
        </fileSet>
        <fileSet>
            <directory>${project.basedir}/MavenService/target</directory>
            <outputDirectory>service</outputDirectory>
            <includes>
                <include>**/*.*</include>
            </includes>
            <fileMode>0755</fileMode>
        </fileSet>
        <fileSet>
            <directory>${project.basedir}/MavenResource/target</directory>
            <outputDirectory>resource</outputDirectory>
            <includes>
                <include>**/*.*</include>
            </includes>
            <fileMode>0755</fileMode>
        </fileSet>
    </fileSets>
</assembly>
